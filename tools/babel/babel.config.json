{
    "plugins": [
        "react-refresh/babel",
        "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
        "@babel/preset-react",
        "@babel/preset-typescript",
        [
            "@babel/preset-env", {
                "targets": {
                    "chrome": "41",
                    "ie": "11",
                    "ios": "9"
                },
                "useBuiltIns": "usage",
                "corejs": 3
            }
        ]
    ]
}